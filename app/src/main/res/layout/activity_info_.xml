<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.profile_activity"
    android:background="@color/menu_bg">

    <ImageView
        android:layout_width="wrap_content"
        android:id="@+id/profile_icnn"
        android:layout_height="wrap_content"
        android:src="@drawable/ifno_icn_about"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="50dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="App Version"
        android:textColor="@color/bottom_view_header_text"
        android:layout_below="@+id/profile_icnn"
        android:layout_centerInParent="true"
        android:layout_margin="10dp"
        android:fontFamily="@font/kanit_regular"
        android:id="@+id/emailcontainer"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1.00"
        android:textColor="@color/white"
        android:layout_below="@+id/emailcontainer"
        android:layout_centerInParent="true"
        android:id="@+id/emai_real_holder"
        android:fontFamily="@font/kanit_medium"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Release Date"
        android:textColor="@color/bottom_view_header_text"
        android:layout_below="@+id/emai_real_holder"
        android:layout_centerInParent="true"
        android:layout_margin="10dp"
        android:fontFamily="@font/kanit_regular"
        android:id="@+id/user_id_holder"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5/7/21"
        android:textColor="@color/white"
        android:layout_below="@+id/user_id_holder"
        android:layout_centerInParent="true"
        android:id="@+id/user_id_container"
        android:fontFamily="@font/kanit_medium"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"/>





</RelativeLayout>